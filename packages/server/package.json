{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "develop": "node_modules/.bin/nodemon --exec babel-node ./src/bin/www",
    "start": "node ./build/bin/www.js"
  },
  "dependencies": {
    "await-exec": "^0.1.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.0",
    "concurrently": "^6.2.1",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.17.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.12",
    "morgan": "~1.9.1",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "nodemon": "^2.0.7"
  },
  "engines": {
    "node": "16.x"
  }
}
